{
  "NServiceBus": {
    "EndpointName": "proto-router"
  },
  "ConnectionStrings": {
    "NServiceBus": {
      "AzureServiceBusA": "",
      "AzureServiceBusB": ""
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "C:\\temp\\logs\\esb\\proto-router-log-{Date}.txt",
                "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                "fileSizeLimitBytes": 20971520,
                "retainedFileCountLimit": 10
              }
            }
          ]
        }
      }
    ]
  }  
}